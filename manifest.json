{
    "manifest_version": 3,
    "name": "Gemini Watermark Remover",
    "version": "1.0.0",
    "description": "Automatically removes watermarks from Gemini AI generated images when downloading",
    "author": "journey-ad",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "host_permissions": [
        "https://gemini.google.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://gemini.google.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start",
            "world": "MAIN"
        }
    ]
}